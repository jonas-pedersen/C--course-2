<Page
    x:Class="FF1.MainPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:FF1"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">

    <Grid>
        <Button x:Name="MyButton" Content="Tryck på mig!" Height="145" Margin="150,268,0,0" VerticalAlignment="Top" Width="248" Click="Button_Click"/>
        <CheckBox x:Name="MyCheckBox" Content="CheckBox" Margin="443,268,0,0" VerticalAlignment="Top" Click="CheckBox_Click" IsChecked="True"/>
        <TextBox x:Name="MyTextBox" HorizontalAlignment="Left" Margin="443,322,0,0" Text="Hallå där" TextWrapping="Wrap" VerticalAlignment="Top" Height="91" Width="457"/>

    </Grid>
</Page>
